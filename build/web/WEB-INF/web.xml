<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>LoginReg.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginReg.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportServlet</servlet-name>
        <servlet-class>ReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Medicine</servlet-name>
        <servlet-class>Medicie.Medicine</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AmbulanceBookingServlet</servlet-name>
        <servlet-class>AmbulanceBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SlotBookingServlet</servlet-name>
        <servlet-class>Booking.SlotBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AmbulServlet</servlet-name>
        <servlet-class>AmbulaceBooking.AmbulServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportServlet</servlet-name>
        <url-pattern>/ReportServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Medicine</servlet-name>
        <url-pattern>/Medicine</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AmbulanceBookingServlet</servlet-name>
        <url-pattern>/AmbulanceBookingServlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SlotBookingServlet</servlet-name>
        <url-pattern>/SlotBookingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AmbulServlet</servlet-name>
        <url-pattern>/AmbulServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
